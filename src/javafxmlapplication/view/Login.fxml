      <VBox alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="122.0" prefWidth="261.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <TextField fx:id="login_nickname" maxWidth="350.0" prefWidth="300.0" promptText="Nombre de usuario" styleClass="inputStyled">
