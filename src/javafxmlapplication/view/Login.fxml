                  <Insets left="20.0" right="20.0" />
               </VBox.margin></TextField>
            <Button fx:id="loginBtn" alignment="CENTER" contentDisplay="CENTER" minHeight="32.0" mnemonicParsing="false" onAction="#loginUser" prefHeight="32.0" prefWidth="200.0" styleClass="loginBtnStyled" text="Iniciar Sesión" textAlignment="CENTER" />
